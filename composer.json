{
    "name": "nonamich/roadmap-php-ecommerce-api",
    "version": "0.0.0",
    "require": {
        "laravel/framework": "^11.31",
        "laravel/tinker": "^2.9",
        "php": "^8.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^3.7",
        "pestphp/pest-plugin-laravel": "^3.0",
        "symplify/monorepo-builder": "^11.2"
    },
    "replace": {
        "laravel/laravel": "self.version"
    },
    "minimum-stability": "a",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "apps/gateway/app/",
            "Database\\Factories\\": "apps/gateway/database/factories/",
            "Database\\Seeders\\": "apps/gateway/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "apps/gateway/tests/"
        }
    },
    "extra": {
        "laravel": []
    }
}
